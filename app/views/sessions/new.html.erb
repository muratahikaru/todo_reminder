<h1>Login</h1>
<%= form_with(url: login_path, scope: :session, local: true) do |f| %>
 <div>
   <%= f.label :email %>
   <%= f.email_field :email %>
 </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.submit "Log in" %>
    <%= link_to "sign up!", new_user_path %>
  </div>
<% end %>
